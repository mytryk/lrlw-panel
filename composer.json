{
    "name": "mytryk/laravel-easypanel",
    "license": "MIT",
    "description": "An admin panel based on livewire",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "rezaamini-ir/laravel-easyblade" : "^0.2.2",
        "laravel/framework": "^8.0",
        "livewire/livewire": "^2.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "EasyPanel\\" : "src/"
        },
        "files": ["src/helpers.php"]
    },
    "extra": {
        "laravel" : {
            "providers" : [
                "EasyPanel\\EasyPanelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
